@model List<AppRole>

    <h5><a asp-action="Create" asp-controller="Role">Rol ekle</a></h5>
    <table class="table table-hover">
        <thead>
            <tr>
                <td>ID</td>
                <td>Rol ismi</td>
            </tr>
        </thead>

        <tbody>
           @foreach (AppRole item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }

        </tbody>
    </table>